  {
    "id": "codeStatement",
    "label": "Code Statement",
    "kind": "ScriptNode",
    "category": "Logic",
    "ports": {
      "inputs": [
        "trigger"
      ],
      "outputs": [
        "code"
      ]
    },
    "ui": {
      "controls": [
        {
          "type": "select",
          "key": "statement",
          "label": "type",
          "default": "if",
          "options": [
            {
              "value": "if",
              "label": "if"
            },
            {
              "value": "ifelse",
              "label": "if / else"
            },
            {
              "value": "elseif",
              "label": "else if"
            },
            {
              "value": "else",
              "label": "else"
            },
            {
              "value": "while",
              "label": "while"
            },
            {
              "value": "for",
              "label": "for"
            },
            {
              "value": "forof",
              "label": "for...of"
            },
            {
              "value": "forin",
              "label": "for...in"
            },
            {
              "value": "switch",
              "label": "switch"
            },
            {
              "value": "try",
              "label": "try / catch"
            },
            {
              "value": "return",
              "label": "return"
            },
            {
              "value": "break",
              "label": "break"
            },
            {
              "value": "continue",
              "label": "continue"
            }
          ]
        },
        {
          "type": "text",
          "key": "condition",
          "label": "condition",
          "default": "true",
          "showWhen": {
            "key": "statement",
            "in": [
              "if",
              "ifelse",
              "elseif",
              "while",
              "for",
              "forof",
              "forin",
              "switch"
            ]
          }
        },
        {
          "type": "text",
          "key": "body",
          "label": "body",
          "default": "/* code */",
          "showWhen": {
            "key": "statement",
            "in": [
              "if",
              "ifelse",
              "else",
              "while",
              "for",
              "forof",
              "forin",
              "switch",
              "try"
            ]
          }
        },
        {
          "type": "text",
          "key": "elsebody",
          "label": "else",
          "default": "/* code */",
          "showWhen": {
            "key": "statement",
            "in": [
              "ifelse",
              "try"
            ]
          }
        },
        {
          "type": "text",
          "key": "returnval",
          "label": "value",
          "default": "",
          "showWhen": {
            "key": "statement",
            "in": [
              "return"
            ]
          }
        }
      ]
    },
    "script": {
      "slotDefault": "update",
      "slotMode": "byTargetPort",
      "templateByAction": {
        "if": "if ({{condition}}) { {{body}} }",
        "ifelse": "if ({{condition}}) { {{body}} } else { {{elsebody}} }",
        "elseif": "else if ({{condition}}) { {{body}} }",
        "else": "else { {{body}} }",
        "while": "while ({{condition}}) { {{body}} }",
        "for": "for ({{condition}}) { {{body}} }",
        "forof": "for (var _item of {{condition}}) { {{body}} }",
        "forin": "for (var _key in {{condition}}) { {{body}} }",
        "switch": "switch ({{condition}}) { {{body}} }",
        "try": "try { {{body}} } catch(e0) { {{elsebody}} }",
        "return": "return {{returnval}};",
        "break": "break;",
        "continue": "continue;"
      }
    }
  },
